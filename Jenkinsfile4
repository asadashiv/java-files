pipeline{
environment {
GIT_BRANCH ='branch2'
}

agent {label 'master'}
stages{
stage ('build'){
steps {
sh '''
cd /var/lib/jenkins/workspace/java3
mvn clean install
'''
}
}

stage ('Deploy'){

when {
  expression {env.GIT_BRANCH ==/(master|branch2)/}
  }
steps {
timeout(1) {
sh '''
 echo "this is deploy stage"
 '''
}
}
}
}
}
