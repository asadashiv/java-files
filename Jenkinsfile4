pipeline{
environment {
GIT_BRANCH ='branch2'
}
agent {label 'master'}
stages{
stage ('build'){
steps {
sh '''
cd /var/lib/jenkins/workspace/java3
mvn clean install
'''
}
}

stage ('deploy'){
parameter {
choice choices: ['1', '2'], name: 'choice'
}
when {
  branch "${env.'branch2'}"
}
steps {
echo "this is deploy stage"
}
}
}
}
