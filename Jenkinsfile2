pipeline{
agent {label 'master'}
stages{
stage ('build'){
steps {
sh '''
cd /var/lib/jenkins/workspace/java2
mvn clean install
'''
}
post {
success{
archiveArtifacts artifacts: '*.war', followSymlinks: false
}
}
}
stage ('copy'){
steps {
sh 'scp -r /var/lib/jenkins/workspace/java2/target/hello-world-war-1.0.0 ubuntu@172.31.4.173:/home/ubuntu/'
}
}
}

}
