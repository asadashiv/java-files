pipeline{
agent {label 'master'}
stages{
stage ('build'){
steps {
sh '''
cd /var/lib/jenkins/workspace/java3
mvn clean install
}
'''
post {
success {
archiveArtifacts artifacts: 'target/*.war', followSymlinks: false
}
}

}

stage ('deploy'){
when {
  branch 'branch2'
}

steps {
sh 'echo "this is deploy stage"'
}

}
}
}
